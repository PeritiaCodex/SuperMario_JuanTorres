.contenedor {
    width: 100%;
    height: 100%;
    background-color: rgb(70, 70, 70);
    overflow: hidden;
    text-overflow: clip;
    position: relative;
}

.titulo {
    font-size: 80px;
    color: rgb(200, 162, 44);
    top: 100px;
}

.creditos {
    top: 200px;
    color: rgb(200, 162, 44);
    font-size: 35px;
    white-space: normal;
}

.boton {
    width: 100px;
    height: 100px;
    position: absolute;
    right: 50px;
    top: 50px;
    background-image: url("project://database/Assets/Saurabh_Studio/UI%20button%20and%20hallowing%20element%20pack%205/gold/CLOSE-1.png?fileID=2800000&guid=c0b6c38e965c69c4baaec76c33e68cf3&type=3#CLOSE-1");
    background-color: rgba(0, 0, 0, 0);
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
}
