.contenedor {
    width: 100%;
    height: 100%;
    align-self: center;
    justify-content: center;
    align-items: center;
}

.contenedorBotones {
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    width: 1200px;
    height: 500px;
    left: 400px;
    right: 400px;
}

.boton {
    width: 400px;
    height: 200px;
    background-image: url("project://database/Assets/Saurabh_Studio/UI%20button%20and%20hallowing%20element%20pack%205/Button%206/7.png?fileID=2800000&guid=d3dd507249881854fbaa9d44a29f0d89&type=3#7");
    background-color: rgba(0, 0, 0, 0);
    border-left-color: rgba(76, 76, 76, 0);
    border-right-color: rgba(76, 76, 76, 0);
    border-top-color: rgba(76, 76, 76, 0);
    border-bottom-color: rgba(76, 76, 76, 0);
    font-size: 50px;
    -unity-font-style: bold;
    color: rgb(255, 255, 255);
}

.grupobotones {
    background-color: rgba(0, 0, 0, 0.37);
    border-top-left-radius: 30px;
    border-top-right-radius: 30px;
    border-bottom-right-radius: 30px;
    border-bottom-left-radius: 30px;
    justify-content: flex-end;
    align-items: center;
    align-self: auto;
    width: 1400px;
    height: 600px;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    background-size: 100% 100%;
    top: 0;
}

.titulo {
    font-size: 80px;
    background-color: rgba(0, 0, 0, 0.27);
    color: rgb(255, 255, 255);
    -unity-font-style: bold;
    bottom: auto;
    border-top-left-radius: 30px;
    border-top-right-radius: 30px;
    border-bottom-right-radius: 30px;
    border-bottom-left-radius: 30px;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 20px;
    border-bottom-width: 20px;
    width: 1800px;
    justify-content: space-evenly;
    align-self: auto;
    top: 0;
    position: relative;
    align-items: auto;
    align-content: flex-start;
    margin-bottom: 70px;
    margin-top: -50px;
}
